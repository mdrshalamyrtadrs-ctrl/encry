{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/data/.*",
      "dest": "/api/no-access"
    },
    {
      "src": "/organized_output.*\\.json",
      "dest": "/api/no-access"
    },
    {
      "src": "/\\.(.*)$",
      "dest": "/api/no-access"
    },
    {
      "src": "/api/courses",
      "dest": "/api/no-access",
      "headers": {
        "x-blocked-reason": "direct-access-forbidden"
      }
    },
    {
      "src": "/api/stream",
      "dest": "/api/no-access"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}